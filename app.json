{
  "name": "http-proxy-cache",
  "description": "A caching HTTP forward proxy",
  "repository": "https://github.com/watson/http-proxy-cache",
  "env": {
    "OPBEAT_ORGANIZATION_ID": {
      "description": "Opbeat organization id (optional)",
      "required": false
    },
    "OPBEAT_APP_ID": {
      "description": "Opbeat app id (optional)",
      "required": false
    },
    "OPBEAT_SECRET_TOKEN": {
      "description": "Opbeat secret token (optional)",
      "required": false
    }
  },
  "addons": [
    "mongolab"
  ]
}
